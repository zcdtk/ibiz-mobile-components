  "devDependencies": {
    "@stencil/core": "^1.13.0",
    "@stencil/sass": "^1.3.1",
    "@types/jest": "^25.2.1",
    "sass": "^1.26.5",
    "tslint-react": "^5.0.0",
    "typescript": "^3.8.3"
  },
  "license": "MIT",
  "dependencies": {
    "@ionic/core": "^5.1.1",
    "ionicons": "^5.0.1",
    "moment": "^2.26.0"
  }
